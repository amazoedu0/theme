<div class="header-main {{ layout_file }} d-none d-xl-block">
	<div class="container">
		<div class="row align-items-xl-center justify-content-between" style="min-height: {{ ss_item.htheight | append: "px" }}">
			<div class="col-auto d-xl-flex align-items-xl-center justify-content-start">
				<div class="item_left_spacer" data-responsive-id="logo-shop" data-responsive-layout="desktop">
					{% include 'header-logo' %}
				</div>
				{% unless topbar %}
					{% if ss_item.use_contactinfo %}
						<div class="contact_phone pl-xl-5 d-flex flex-column item_left_spacer">
							{% if ss_item.contacts_phone != blank %}
								<a class="tel_ltr d-inline-flex align-items-center" href="tel:{{ ss_item.contacts_phone }}">
									<svg class="svg-icon" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
										<path d="M9.366 10.682C10.3043 12.3305 11.6695 13.6957 13.318 14.634L14.202 13.396C14.3442 13.1969 14.5543 13.0569 14.7928 13.0023C15.0313 12.9478 15.2814 12.9825 15.496 13.1C16.9103 13.8729 18.4722 14.3378 20.079 14.464C20.3298 14.4839 20.5638 14.5975 20.7345 14.7823C20.9052 14.9671 21 15.2094 21 15.461V19.923C21.0001 20.1706 20.9083 20.4094 20.7424 20.5932C20.5765 20.777 20.3483 20.8927 20.102 20.918C19.5698 20.9729 19.0351 21.0003 18.5 21C9.94 21 3 14.06 3 5.5C3 4.962 3.027 4.428 3.082 3.898C3.10725 3.6517 3.22298 3.42352 3.40679 3.25763C3.5906 3.09175 3.82941 2.99995 4.077 3H8.539C8.79056 2.99997 9.0329 3.09475 9.21768 3.26545C9.40247 3.43615 9.51613 3.67022 9.536 3.921C9.66222 5.52779 10.1271 7.08968 10.9 8.504C11.0175 8.71856 11.0522 8.96874 10.9977 9.2072C10.9431 9.44565 10.8031 9.65584 10.604 9.798L9.366 10.682ZM6.844 10.025L8.744 8.668C8.20478 7.50409 7.83535 6.26884 7.647 5H5.01C5.004 5.166 5.001 5.333 5.001 5.5C5 12.956 11.044 19 18.5 19C18.667 19 18.834 18.997 19 18.99V16.353C17.7312 16.1646 16.4959 15.7952 15.332 15.256L13.975 17.156C13.4287 16.9437 12.898 16.6931 12.387 16.406L12.329 16.373C10.3676 15.2567 8.74328 13.6324 7.627 11.671L7.594 11.613C7.30691 11.102 7.05628 10.5713 6.844 10.025Z"/>
									</svg>
									<span>{{ ss_item.contacts_phone }}</span>
								</a>
							{% endif %}
							{% if ss_item.contacts_text != blank %}
								<span class="phone_info ml-2">{{ ss_item.contacts_text }}</span>
							{% endif %}
						</div>
					{% endif %}
				{% endunless  %}
			</div>
			
			<div class="col-auto d-flex justify-content-end align-items-center position-static header_items_wrapper">
              
              {% unless topbar and ss_item.header_tb_cs %}
					{%- if ss_item.use_currency_selector  -%}
						<div class="col position-static d-flex justify-content-end">
							<div class="item_left_spacer" data-responsive-id="currency-selector" data-responsive-layout="desktop">
								{% include "header-currency" %}
							</div>
						</div>
					{%- endif -%}
				{% endunless %}
				
				{% if shop.customer_accounts_enabled and ss_item.header_user %}
					<div class="item_left_spacer" data-responsive-id="header-account" data-responsive-layout="desktop">
						{% include 'header-account' %}
					</div>
				{% endif %}
				
				
				{% if ss_item.enable_header_wishlist %}
					<div class="item_left_spacer" data-responsive-id="wishlist" data-responsive-layout="desktop">
						{% include "header-wishlist" %}
					</div>
				{% endif %}
				
				{% if ss_item.header_cart %}
					<div class="item_left_spacer" data-responsive-id="shopping-cart" data-responsive-layout="desktop">
						{%- assign ajax_cart = ss_item.ajax_cart -%}
						{% include 'header-cart' ajax: ajax_cart, style: 'type_2' %}
					</div>
				{% endif %}
			</div>
		</div>
	</div>
	<div class="header-menu-custom {% if ss_item.inverted_ms_h %} inverted{% endif %}"  style="background-color: {{ ss_item.bg_ms_h }}; min-height: {{ ss_item.h_ms_height | append: "px" }}">
		<div class="container">
			<div class="row align-items-xl-center justify-content-between" style="min-height: {{ ss_item.h_ms_height | append: "px" }}">
				<div class="col position-static d-flex justify-content-{{ ss_item.megamenu_position }}">
					{% if ss_item.megamenu_toggle %}
						<div data-responsive-id="megamenu" data-responsive-layout="desktop">
							{% if ss_item.menu_burger %}
								<div class="d-xl-block d-none">
									{% include 'burger-megamenu' %}
								</div>
								<div class="d-xl-none d-block">
									{% include 'main-megamenu' %}
								</div>
							{% else %}
								{% include 'main-megamenu' %}
							{% endif %}
						</div>
					{% else %}
						{%- if ss_item.default_menu != blank -%}
							<div data-responsive-id="default-menu" data-responsive-layout="desktop">
								{% if ss_item.menu_burger %}
									<div class="d-xl-block d-none">
										{% include 'burger-defmenu' %}
									</div>
									<div class="d-xl-none d-block">
										{% include 'default-menu' %}
									</div>
								{% else %}
									{% include 'default-menu' %}
								{% endif %}

							</div>
						{%- endif -%}
					{% endif %}

				</div>
              {% if ss_item.header_search %}
					<div class="item_left_spacer col d-flex justify-content-end{% if toggle_search == false %} mr-xl-2{% endif %}" data-responsive-id="header-search" data-responsive-layout="desktop">
						{% if toggle_search %}
							{% include 'header-search' %}
						{% else %}
							{% include "search-content" %}
						{% endif %}
					</div>
				{% endif %}
			</div>
		</div>
	</div>
</div>







