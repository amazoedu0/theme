{{ 'section-image-text-column.css' | asset_url | stylesheet_tag }}


{% style %}
	/* Style inset */
	.section-style-{{ section.id }} .image-section-wrap {
		padding: {{ section.settings.inset_image }};
	}
	.section-style-{{ section.id }} .caption {
		padding: {{ section.settings.inset_caption }};
	}
	
	
	/* Style background */
	{% if section.settings.use_bgimg %}
		.section-style-{{ section.id }}  .image-section-wrap {
			background-color: {{ section.settings.image_bg_color | color_modify: 'alpha', section.settings.image_bg_color_opacity }};
		}
	{% endif %}
	{% if section.settings.use_caption_bg %}
		.section-style-{{ section.id }} .caption {
			background-color: {{ section.settings.caption_bg_color | color_modify: 'alpha', section.settings.caption_bg_color_opacity }};
		}
	{% endif %}
	
	{% if section.blocks.size > 0 %}
		{% for block in section.blocks %}


			.section-style-{{ section.id }} .item-{{ block.id }} {
				margin: {{ block.settings.offset_block }};
			}
			.section-style-{{ section.id }} .item-{{ block.id }} .caption-item,
			.section-style-{{ section.id }} .item-{{ block.id }}.block-timer {
				color: {{ block.settings.color }};
				padding: {{ block.settings.inset_block }};
				font-weight: {{ block.settings.font_weight }};
				{% if block.settings.use_bgi %}
					background-color: {{ block.settings.caption_bg_color | color_modify: 'alpha', block.settings.caption_bg_color_opacity }};
				{% endif %}
			}
			
			{% if block.settings.indent_block != '0' and block.settings.indent_block != blank %}
				@media (min-width: 768px)  {
					.section-style-{{ section.id }} .item-{{ block.id }}{
						text-indent: {{ block.settings.indent_block }}; 
					}
				}
			{% endif %}
		

			@media screen and (min-width: 1200px) { 
				.section-style-{{ section.id }} .item-{{ block.id }}.item-fz{
					font-size: {{ block.settings.heading_size | append: 'px' }};   
				}
  			}


		{% endfor %}
	{% endif %}
{% endstyle %}



<div class="section-style-{{ section.id }}">
	{%- render 'section-style' -%}
	
	<div class="{{ section.settings.container_width }}">
		<div class="row flex-{{ section.settings.column_dir }} flex-md-nowrap justify-content-between {% if section.settings.reset_indent %}gx-0{% endif %}">
			<div class="col-md-{{ section.settings.image_column }}">
				<div class="image-section-wrap">
					{% if section.settings.image != blank %}
						<div class="image-item-wrapper" style="padding-top:{{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;">
							{% assign img_url = section.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
							<img loading="lazy" class="image-content__image lazyload"
								 src="{{ section.settings.image | img_url: '300x300' }}"
								 data-src="{{ img_url }}"
								 data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
								 data-aspectratio="{{ section.settings.image.aspect_ratio }}"
								 data-sizes="auto"
								 data-parent-fit="cover"
								 alt="{{ section.settings.image.alt | escape }}"/>
						</div>
					{% else %}
						<div class="image-item-container">
							<div class="img_placeholder__wrap" style="padding-top: 52.6%;">
								{{ 'image' | placeholder_svg_tag: 'img_placeholder' }}
							</div>
						</div>
					{% endif %}
				</div>
			</div>
			<div class="col-md-{{ section.settings.caption_column }}">
				<div class="caption d-flex flex-column h-100 justify-content-{{ section.settings.caption_align_v }}">
					<div class="caption_text text-{{ section.settings.text_align }} {% if section.settings.inverted %} inverted{% endif %}">
						{% for block in section.blocks %}
							{% case block.type %}
							{% when 'custom_heading' %}
								{% if block.settings.custom_heading != blank %}
									<div class="item-fz custom_large_title item-{{ block.id }} wow {{ block.settings.title_animation }}"><span class="caption-item">{{ block.settings.custom_heading }}</span></div>
								{% endif %}
							{% when 'heading_h1' %}
								{% if block.settings.heading_h1 != blank %}
									<h1 class="item-fz item-{{ block.id }} wow {{ block.settings.title_animation }}"><span class="caption-item">{{ block.settings.heading_h1 }}</span></h1>
								{% endif %}
							{% when 'heading_h2' %}
								{% if block.settings.heading_h2 != blank %}
									<h2 class="item-fz item-{{ block.id }}"><span class="caption-item">{{ block.settings.heading_h2 }}</span></h2>
								{% endif %}
							{% when 'heading_h3' %}
								{% if block.settings.heading_h3 != blank %}
									<h3 class="item-fz item-{{ block.id }} wow {{ block.settings.title_animation }}"><span class="caption-item">{{ block.settings.heading_h3 }}</span></h3>
								{% endif %}
							{% when 'heading_h4' %}
								{% if block.settings.heading_h4 != blank %}
									<h4 class="item-fz item-{{ block.id }} wow {{ block.settings.title_animation }}"><span class="caption-item">{{ block.settings.heading_h4 }}</span></h4>
								{% endif %}
							{% when 'heading_h5' %}
								{% if block.settings.heading_h5 != blank %}
									<h5 class="item-fz item-{{ block.id }} wow {{ block.settings.title_animation }}"><span class="caption-item">{{ block.settings.heading_h5 }}</span></h5>
								{% endif %}
							{% when 'heading_h6' %}
								{% if block.settings.heading_h6 != blank %}
									<h6 class="item-fz item-{{ block.id }} wow {{ block.settings.title_animation }}"><span class="caption-item">{{ block.settings.heading_h6 }}</span></h6>
								{% endif %}
							{% when 'richtext' %}
								{% if block.settings.subtitle != blank %}
									<div class="item-fz item-{{ block.id }} wow {{ block.settings.title_animation }}"><div class="caption-item">{{ block.settings.subtitle }}</div></div>
								{% endif %}
							{% when 'text' %}
								{% if block.settings.text != blank %}
									<div class="item-fz item-{{ block.id }} wow {{ block.settings.title_animation }}"><span class="caption-item">{{ block.settings.text }}</span></div>
								{% endif %}
							{% when 'timer' %}
								{%- if block.settings.time != blank -%}
									<div class="item-fz js-countdown h5 block-timer item-{{ block.id }} wow {{ block.settings.title_animation }}" data-time="{{ block.settings.time }}"></div>
								{%- endif -%} 
							{% when 'button' %}
								{% if block.settings.button_label.size > 0 and block.settings.button_link.size > 0 %}
									<a href="{{ block.settings.button_link }}" class="{% if block.settings.use_button %}btn {{ block.settings.btn_style }} {{ block.settings.btn_size }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block }};">{{ block.settings.button_label }}</a>
								{% endif %}
							{% when 'groupButtons' %}
				
									<div class="groupButtons" style="display: table-caption;margin: {{ block.settings.offset_block_general }};width: {{ block.settings.block_width }}%;">
									  <ul style="column-count: {{ block.settings.column_count }};">
										
										{% if block.settings.button_label_1.size > 0 and block.settings.button_link_1.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_1 }}" class="{% if block.settings.use_button_1 %}btn {{ block.settings.btn_style_1 }} {{ block.settings.btn_size_1 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_1 }};">{{ block.settings.button_label_1 }}</a>
										  </li>
										{% endif %}
										
										{% if block.settings.button_label_2.size > 0 and block.settings.button_link_2.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_2 }}" class="{% if block.settings.use_button_2 %}btn {{ block.settings.btn_style_2 }} {{ block.settings.btn_size_2 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_2 }};">{{ block.settings.button_label_2 }}</a>
										  </li>
										{% endif %}
										
										{% if block.settings.button_label_3.size > 0 and block.settings.button_link_3.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_3 }}" class="{% if block.settings.use_button_3 %}btn {{ block.settings.btn_style_3 }} {{ block.settings.btn_size_3 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_3 }};">{{ block.settings.button_label_3 }}</a>
										  </li>
										{% endif %}
		
										{% if block.settings.button_label_4.size > 0 and block.settings.button_link_4.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_4 }}" class="{% if block.settings.use_button_4 %}btn {{ block.settings.btn_style_4 }} {{ block.settings.btn_size_4 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_4 }};">{{ block.settings.button_label_4 }}</a>
										  </li>
										{% endif %}
										
										{% if block.settings.button_label_5.size > 0 and block.settings.button_link_5.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_5 }}" class="{% if block.settings.use_button_5 %}btn {{ block.settings.btn_style_5 }} {{ block.settings.btn_size_5 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_5 }};">{{ block.settings.button_label_5 }}</a>
										  </li>
										{% endif %}
										
										{% if block.settings.button_label_6.size > 0 and block.settings.button_link_6.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_6 }}" class="{% if block.settings.use_button_6 %}btn {{ block.settings.btn_style_6 }} {{ block.settings.btn_size_6 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_6 }};">{{ block.settings.button_label_6 }}</a>
										  </li>
										{% endif %}
										{% if block.settings.button_label_7.size > 0 and block.settings.button_link_7.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_7 }}" class="{% if block.settings.use_button_7 %}btn {{ block.settings.btn_style_7 }} {{ block.settings.btn_size_7 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_7 }};">{{ block.settings.button_label_7 }}</a>
										  </li>
										{% endif %}
										{% if block.settings.button_label_8.size > 0 and block.settings.button_link_8.size > 0 %}
										  <li>
											<a href="{{ block.settings.button_link_8 }}" class="{% if block.settings.use_button_8 %}btn {{ block.settings.btn_style_8 }} {{ block.settings.btn_size_8 }}{% else %}link link-primary {% endif %}" style="margin: {{ block.settings.offset_block_8 }};">{{ block.settings.button_label_8 }}</a>
										  </li>
										{% endif %}
									  </ul>
									</div>
								  
							{% when 'link_list' %}
								   {%- assign list = block.settings.links -%}
									<div class="linklist item-{{ block.id }}">
									  <ul class="linklist--inner">
										{% for link in list.links %}
										  <li class="d-inline-block mt-1 mr-1">
											<a class="{% if block.settings.use_button %}btn {{ block.settings.btn_style }} {{ block.settings.btn_size }}{% else %}link link-primary {% endif %}" href="{{ link.url }}">
											  <span>{{ link.title }}</span>
											</a>
										  </li>
										{% endfor %}
									  </ul>
									</div>
							{% when 'html' %}
								{% if block.settings.code.size != blank %}
									<div class="custom_html item-{{ block.id }}">
										{{ block.settings.code }}
									</div> 
								{% endif %}
							{% when 'custom_liquid' %}
								{% if block.settings.custom_liquid != blank %}
									<div class="custom_liquid item-{{ block.id }}">
										{{ block.settings.custom_liquid }}
									</div>
								{% endif %}
							{% endcase %}
						{% endfor %}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

{% schema %}
{
	"name": "Image with text column",
	"tag": "section",
	"class": "section section_homepage section_image-with-text-column",
	"max_blocks": 8,
	"settings": [
		{
			"type": "header",
			"content": "Section sittings"
		},
		{
			"type":      "select",
			"id":        "container_width",
			"label":     "Container width",
			"options": [
				{
					"value": "container",
					"label": "Box"
				},
				{
					"value": "container-fluid",
					"label": "Container Fluid"
				},
				{
					"value": "full-width",
					"label": "Full"
				}
			],
				"default":   "container"
		},
	
		{
			"type": "checkbox",
			"id": "reset_indent",
			"label": "Remove indent",
			"default":   false
		},

		{
			"type": "select",
			"id": "column_dir",
			"label": "Column direction",
			"default": "row",
			"info": "Image left or right side",
			"options": [
				{
					"value": "row",
					"label": "Image left"
				},
				{
					"value": "row-reverse",
					"label": "Image right"
				}
			]
		},
		
		{
			"type": "text",
			"id": "inset_section",
			"label": "Inset Section",
			"default": "0px 0px 0px 0px",
			"info": "inset: Top Right Bottom Left (px/em/%/). Or use one value if all insets are equal"
		},

		{
			"type": "text",
			"id": "offset_section",
			"label": "Offset Section",
			"default": "0px 0px 0px 0px",
			"info": "offset: Top Right Bottom Left"
		},

		{
			"type": "color",
			"id": "bg_section",
			"label": "Background Section",
			"default": "#ffffff"
		},

		{
			"type": "header",
			"content": "Image settings"
		},
	
		{
			"type": "image_picker",
			"id": "image",
			"label": "Image",
			"info": "The image should be more than 2048px wide to resize and crop properly"
		},

		{
			"type":      "select",
			"id":        "image_column",
			"label":     "Image Column width",
			"default": "6",
			"options": [
				{
					"value": "2",
					"label": "16.67%"
				},
				{
					"value": "3",
					"label": "25%"
				},
				{
					"value": "4",
					"label": "33.33%"
				},
				{
					"value": "5",
					"label": "41.67%"
				},
				{
					"value": "6",
					"label": "50%"
				},
				{
					"value": "7",
					"label": "58.33%"
				},
				{
					"value": "8",
					"label": "66.67%"
				},
				{
					"value": "9",
					"label": "75%"
				}
				
			]
		},

		{
			"type": "checkbox",
			"id": "use_bgimg",
			"label": "Use background image",
			"default": false,
			"info": "this option together with 'Inset Image' will help you create a border around the picture"
		},
		{
			"type": "color",
			"id": "image_bg_color",
			"label": "Caption background"
		},
		{
			"type":      "range",
			"id":        "image_bg_color_opacity",
			"min":       0,
			"max":        1,
			"step":       0.1,
			"unit":       "px",
			"label":     "Background caption opacity",
			"default":   0.5
		},

		{
			"type": "text",
			"id": "inset_image",
			"label": "Inset Image",
			"default": "0px 0px 0px 0px",
			"info": "inset: Top Right Bottom Left (px/em/%/). Or use one value if all insets are equal"
		},

		{
			"type": "header",
			"content": "Caption Settings"
		},

		{
			"type":      "select",
			"id":        "caption_column",
			"label":     "Caption column width",
			"default": "6",
			"options": [
				{
					"value": "2",
					"label": "16.67%"
				},
				{
					"value": "3",
					"label": "25%"
				},
				{
					"value": "4",
					"label": "33.33%"
				},
				{
					"value": "5",
					"label": "41.67%"
				},
				{
					"value": "6",
					"label": "50%"
				},
				{
					"value": "7",
					"label": "58.33%"
				},
				{
					"value": "8",
					"label": "66.67%"
				},
				{
					"value": "9",
					"label": "75%"
				}
			]
		},

		{
			"type": "select",
			"id": "caption_align_v",
			"label": "Caption vertical align",
			"default": "center",
			"options": [
				{
					"label": "Center",
					"value": "center"
				},
				{
					"label": "Bottom",
					"value": "end"
				},
				{
					"label": "Top",
					"value": "start"
				}
			]
		},

		{
			"type": "select",
			"id": "text_align",
			"label": "Text align",
			"default": "center",
				"options": [
					{
						"label": "Center",
						"value": "center"
					},
					{
						"label": "Right",
						"value": "right"
					},
					{
						"label": "Left",
						"value": "left"
					}
				]
		},
		{
			"type": "checkbox",
			"id": "inverted",
			"label": "Inverted",
			"default": false
		},
	
		{
			"type": "checkbox",
			"id": "use_caption_bg",
			"label": "Use background caption",
			"default": false
		},
		{
			"type": "color",
			"id": "caption_bg_color",
			"label": "Caption background"
		},
		{
	        "type":      "range",
	        "id":        "caption_bg_color_opacity",
	        "min":       0,
	        "max":        1,
	        "step":       0.1,
	        "unit":       "px",
	        "label":     "Background caption opacity",
	        "default":   0.5
		},
		{
			"type": "text",
			"id": "inset_caption",
			"label": "Inset Caption",
			"default": "6% 7% 6% 7%",
			"info": "inset: Top Right Bottom Left (px/em/%/). Or use one value if all insets are equal"
		}
	],
	"blocks": [
		// CUSTOM HEADING //////////////////////////////////////////////////////////////////////////////////////////
		{
		  "type": "custom_heading",
		  "name": "Custom Heading",
		  "settings": [
			{
			  "type": "text",
			  "id": "custom_heading",
			  "label": "Custom size heading",
			  "default": "Custom Heading"
			},
			{
				"type": "range",
				"id": "heading_size",
				"min": 14,
				"max": 150,
				"step": 2,
				"label": "Heading font size (desktop)",
				"default": 120
			}, 
			{
				"type": "range",
				"id": "font_weight",
				"min": 300,
				"max": 900,
				"step": 100,
				"label": "Font weight",
				"default": 400,
				"info": "The boldness of the font depends on the selected font type."
			},
			{
			  "type": "color",
			  "id": "color",
			  "label": "Color",
			  "default": "#121212"
			},
			{
			  "type": "text",
			  "id": "indent_block",
			  "label": "Indent Block",
			  "default": "0",
			  "info": "Text indent: negative or positive value (px or %)"
			},
			{
			  "type": "text",
			  "id": "inset_block",
			  "label": "Inset Block",
			  "default": "0 0 0 0",
			  "info": "inset: Top Right Bottom Left (px/em/%/). Or use one value if all insets are equal"
			},
			{
			  "type": "text",
			  "id": "offset_block",
			  "label": "Offset Block",
			  "default": "5% 0 0 0",
			  "info": "offset: Top Right Bottom Left"
			},
			{
			  "type": "checkbox",
			  "id": "use_bgi",
			  "label": "Use background caption item",
			  "default": false
			},
			{
			  "type": "color",
			  "id": "bgc_color",
			  "label": "Color background caption item"
			},
			{
			  "type": "range",
			  "id": "bgc_color_opacity",
			  "min": 0,
			  "max": 1,
			  "step": 0.1,
			  "label": "Background opacity caption item",
			  "default": 0.5
			},
			{
			  "type": "header",
			  "content": "Animation Settings"
			},
			{
			  "type": "select",
			  "id": "title_animation",
			  "label": "Animation",
			  "options": [
				{
				  "value": "none",
				  "label": "Don't use animation"
				},
				{
				  "value": "tracking-in-expand",
				  "label": "Style 1"
				},
				{
				  "value": "tracking-in-contract-bck-top",
				  "label": "Style 2"
				},
				{
				  "value": "focus-in-contract",
				  "label": "Style 3"
				},
				{
				  "value": "text-focus-in",
				  "label": "Style 4"
				},
				{
				  "value": "tracking-in-contract-bck",
				  "label": "Style 5"
				},
				{
				  "value": "text-shadow-drop-bottom",
				  "label": "Style 6"
				}
			  ],
			  "default": "none"
			}
		  ]
		},
		// HEADER H1 //////////////////////////////////////////////////////////////////////////////////////////
		{
		  "type": "heading_h1",
		  "name": "Heading H1",
		  "settings": [
			{
			  "type": "text",
			  "id": "heading_h1",
			  "label": "H1",
			  "default": "Heading"
			},
			{
				"type": "range",
				"id": "heading_size",
				"min": 16,
				"max": 80,
				"step": 2,
				"label": "Heading font size (desktop)",
				"default": 72
			},
		    {
				"type": "range",
				"id": "font_weight",
				"min": 300,
				"max": 900,
				"step": 100,
				"label": "Font weight",
				"default": 400,
				"info": "The boldness of the font depends on the selected font type."
			},
			{
			  "type": "color",
			  "id": "color",
			  "label": "Color",
			  "default": "#121212"
			},
			
			{
			  "type": "text",
			  "id": "indent_block",
			  "label": "Indent Block",
			  "default": "0",
			  "info": "Text indent: negative or positive value (px or %)"
			},
			{
			  "type": "text",
