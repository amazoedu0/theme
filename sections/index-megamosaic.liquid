{{ 'section-collection-list.css' | asset_url | stylesheet_tag }}
{{ 'section-featured-products.css' | asset_url | stylesheet_tag }}
{{ 'section-lookbook.css' | asset_url | stylesheet_tag }}
{{ 'section-banners.css' | asset_url | stylesheet_tag }}
{{ 'section-mosaic.css' | asset_url | stylesheet_tag }}
{{ 'component-rating.css' | asset_url | stylesheet_tag }}


<div class="section-style-{{ section.id }} {{ section.settings.design_type }}">
  {%- render 'section-style' -%}
  
  <div class="{{ section.settings.container_width }} container--common">
	
	{% if section.settings.section_title != blank or section.settings.section_subtitle != blank %}
	  <div class="section_heading_wrap mb-3 text-{{ section.settings.sec_title_align }}">
		{% if section.settings.section_title != blank %}
		  <h2 class="h4">{{ section.settings.section_title | escape }}</h2>
		{% endif %}
		{% if section.settings.section_subtitle != blank %}
		  <div class="text mt-1">{{ section.settings.section_subtitle | escape }}</div>
		{% endif %}
	  </div>
	{% endif %}
	
	
	<div class="row{% if section.settings.reset_indent %} gx-0{% endif %}">
	  {% for block in section.blocks %}
	  
	  {%- assign item = block.settings -%}
	  
	  {% case item.item_width %}
	  {% when '1' %}
	  {%- assign item_width = 'col-xl-2' -%}
	  {%- assign item_width_t = 'col-md-2' -%}
	  {%- assign item_width_m = 'col-2' -%}
	  {% when '2' %}
	  {%- assign item_width = 'col-xl-2-5' -%}
	  {%- assign item_width_t = 'col-md-2-5' -%}
	  {%- assign item_width_m = 'col-2-5' -%}
	  {% when '3' %}
	  {%- assign item_width = 'col-xl-3' -%}
	  {%- assign item_width_t = 'col-md-3' -%}
	  {%- assign item_width_m = 'col-3' -%}
	  {% when '4' %}
	  {%- assign item_width = 'col-xl-4' -%}
	  {%- assign item_width_t = 'col-md-4' -%}
	  {%- assign item_width_m = 'col-4' -%}
	  {% when '5' %}
	  {%- assign item_width = 'col-xl-5' -%}
	  {%- assign item_width_t = 'col-md-5' -%}
	  {%- assign item_width_m = 'col-5' -%}
	  {% when '6' %}
	  {%- assign item_width = 'col-xl-6' -%}
	  {%- assign item_width_t = 'col-md-6' -%}
	  {%- assign item_width_m = 'col-6' -%}
	  {% when '7' %}
	  {%- assign item_width = 'col-xl-7' -%}
	  {%- assign item_width_t = 'col-md-7' -%}
	  {%- assign item_width_m = 'col-7' -%}
	  {% when '8' %}
	  {%- assign item_width = 'col-xl-8' -%}
	  {%- assign item_width_t = 'col-md-8' -%}
	  {%- assign item_width_m = 'col-8' -%}
	  {% when '9' %}
	  {%- assign item_width = 'col-xl-9' -%}
	  {%- assign item_width_t = 'col-md-9' -%}
	  {%- assign item_width_m = 'col-9' -%}
	  {% when '10' %}
	  {%- assign item_width = 'col-xl-10' -%}
	  {%- assign item_width_t = 'col-md-10' -%}
	  {%- assign item_width_m = 'col-10' -%}
	  {% when '11' %}
	  {%- assign item_width = 'col-xl-11' -%}
	  {%- assign item_width_t = 'col-md-11' -%}
	  {%- assign item_width_m = 'col-11' -%}
	  {% when '12' %}
	  {%- assign item_width = 'col-xl-12' -%}
	  {%- assign item_width_t = 'col-md-12' -%}
	  {%- assign item_width_m = 'col-12' -%}
	  {% endcase %}
	  
	  {% case item.item_width_t %}
	  {% when '1' %}
	  {%- assign item_width_t = 'col-md-2' -%}
	  {% when '2' %}
	  {%- assign item_width_t = 'col-md-2-5' -%}
	  {% when '3' %}
	  {%- assign item_width_t = 'col-md-3' -%}
	  {% when '4' %}
	  {%- assign item_width_t = 'col-md-4' -%}
	  {% when '5' %}
	  {%- assign item_width_t = 'col-md-5' -%}
	  {% when '6' %}
	  {%- assign item_width_t = 'col-md-6' -%}
	  {% when '7' %}
	  {%- assign item_width_t = 'col-md-7' -%}
	  {% when '8' %}
	  {%- assign item_width_t = 'col-md-8' -%}
	  {% when '9' %}
	  {%- assign item_width_t = 'col-md-9' -%}
	  {% when '10' %}
	  {%- assign item_width_t = 'col-md-10' -%}
	  {% when '11' %}
	  {%- assign item_width_t = 'col-md-11' -%}
	  {% when '12' %}
	  {%- assign item_width_t = 'col-md-12' -%}
	  {% endcase %}
	  
	  {% case item.item_width_m %}
	  {% when '1' %}
	  {%- assign item_width_m = 'col-2' -%}
	  {% when '2' %}
	  {%- assign item_width_m = 'col-2-5' -%}
	  {% when '3' %}
	  {%- assign item_width_m = 'col-3' -%}
	  {% when '4' %}
	  {%- assign item_width_m = 'col-4' -%}
	  {% when '5' %}
	  {%- assign item_width_m = 'col-5' -%}
	  {% when '6' %}
	  {%- assign item_width_m = 'col-6' -%}
	  {% when '7' %}
	  {%- assign item_width_m = 'col-7' -%}
	  {% when '8' %}
	  {%- assign item_width_m = 'col-8' -%}
	  {% when '9' %}
	  {%- assign item_width_m = 'col-9' -%}
	  {% when '10' %}
	  {%- assign item_width_m = 'col-10' -%}
	  {% when '11' %}
	  {%- assign item_width_m = 'col-11' -%}
	  {% when '12' %}
	  {%- assign item_width_m = 'col-12' -%}
	  {% endcase %}
	  
	  <div class="{{ item_width_m }} {{ item_width_t }} {{ item_width }} item-block--{{ block.id }} {% unless item.offset_top_item_block == '0' %} mb-2{% endunless %} mb-xl-{{ item.offset_top_item_block }}">
		<style>
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper {
		  padding: {{ item.inset_item_block }};
		  }
		</style>
		{% if item.block_title != blank or item.block_text != blank %}
		  <div class="section_heading_wrap text-{{ item.title_align }} mb-3">
			{% if item.block_title != blank %}
			  <h2 class="h4">{{ item.block_title | escape }}</h2>
			{% endif %}
			{% if item.block_text != blank %}
			  <div class="text mt-1">{{ item.block_text | escape }}</div>
			{% endif %}
		  </div>
		{% endif %}
		{% case block.type %}
		{% when 'banner' %}
		{% case item.caption_size %}
		{% when 'large' %}
		{%- assign subtitle_class = 'h5' -%}
		{%- assign title_class = 'h1' -%}
		{%- assign text_class = 'text-medium' -%}
		{% when 'medium' %}
		{%- assign subtitle_class = 'h5' -%}
		{%- assign title_class = 'h2' -%}
		{%- assign text_class = 'text-medium' -%}
		{% when 'xsmall' %}
		{%- assign title_class = 'h4' -%}
		{%- assign text_class = 'text-medium' -%}
		  {% else %}
		{%- assign subtitle_class = 'h6' -%}
		{%- assign title_class = 'h3' -%}
		{%- assign text_class = 'text-medium' -%}
		{% endcase %}
		
		<style>
		  
		  {% if item.use_bgo %}
			/* Style background block */
			.section-style-{{ section.id }} .item-block--{{ block.id }} .caption::before {
			content: '';
			position: absolute;
			top: 0;
			left: 0;
			right: 0;
			bottom: 0;
			display: block;
			z-index: 1;
			background-color: {{ item.bg_color | color_modify: 'alpha', item.bg_color_opacity }};
			transition: .3s;
			}
			
			.section-style-{{ section.id }} .item-block--{{ block.id }} .banner-item:hover .caption:before,
			.section-style-{{ section.id }} .item-block--{{ block.id }} .banner-item:focus .caption:before {
			opacity: .75;
			}
		  
		  {% endif %}
		  
		  {% if item.use_bottom_offset_mobile %}
			@media (max-width: 767px) {
			.section-style-{{ section.id }} .item-block--{{ block.id }} {
			margin-bottom: {{ item.margin_bottom | append: 'px' }};
			}
			}
		  
		  {% endif %}
		  
		  /* Style block overlay */
		  {% if item.off_overlay_mobile %}
			@media (min-width: 768px) {
		  {% endif %}
		  
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .style-overlay .banner-caption {
		  position: absolute;
		  left: 0;
		  top: 0;
		  bottom: 0;
		  right: 0;
		  }
		  
		  {% if item.off_overlay_mobile %}
			}
		  
		  {% endif %}
		  
		  /* Style indent for elements */
		  @media (min-width: 1200px) {
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .caption *:not(.btn) + .btn {
		  margin-top: {{ item.margin_btn | append: 'px' }};
		  }
		  
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .caption * + .block-title {
		  margin-top: {{ item.margin_title | append: 'px' }};
		  }
		  
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .caption * + .block-text {
		  margin-top: {{ item.margin_text | append: 'px' }};
		  }
		  }
		  
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .block-title {
		  color: {{ item.color_title }};
		  }
		  
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .custom_large_title.block-title {
		  color: {{ item.color_ch }};
		  }
		  
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .block-text {
		  color: {{ item.color_text }};
		  }
		  
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .block-subtitle {
		  color: {{ item.color_subtitle }};
		  }
		
		</style>
		<div class="banner-item style-{{ item.banner_style }} d-flex flex-column {% if item.reverse_cpos == 'flex-column-reverse' %} flex-md-column-reverse {% endif %}" style="padding: {{ item.inset_block }}; {% if item.use_bgb %}background-color: {{ item.bgb_color | color_modify: 'alpha', item.bgb_color_opacity }};{% endif %}">
		  {% if item.image != blank %}
		  {%- if item.item_link != blank -%}
		  <a class="image-item-container link_hover" href="{{ item.item_link }}">
			{% else %}
			<div class="image-item-container">
			  {%- endif -%}
			  <div class="image-item-wrapper" style="padding-top:{{ 1 | divided_by: item.image.aspect_ratio | times: 100 }}%;">
				{% assign img_url = item.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
				<img loading="lazy" class="image-content__image lazyload"
					 src="{{ item.image | img_url: '300x300' }}"
					 data-src="{{ img_url }}"
					 data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
					 data-aspectratio="{{ item.image.aspect_ratio }}"
					 data-sizes="auto"
					 alt="{{ item.image.alt | escape }}">
			  </div>
			{%- if item.item_link != blank -%}</a> {% else %} </div> {%- endif -%}
	  {% else %}
		<div class="image-item-container">
		  <div class="img_placeholder__wrap" style="padding-top: 52.6%;">
			{{ 'image' | placeholder_svg_tag: 'img_placeholder' }}
		  </div>
		</div>
		{% endif %}
		{% if item.item_subtitle != blank or item.item_title != blank or item.item_text != blank or item.link_text != blank %}
		  <div class="banner-caption {% if item.width == '12' %} container {% endif %} caption caption-{{ item.caption_size }} d-flex flex-column text-md-{{ item.textalign_pos }} justify-content-md-{{ item.v_cpos }} align-items-md-{{ item.h_cpos }} {% if item.inverted %} inverted{% endif %}" style="padding: {{ item.inset_caption }};{% if item.link_type_banner and item.item_link != blank and item.banner_style == 'overlay' %} pointer-events: none;{% endif %}">
			<div class="caption_text">
			  {% if item.item_subtitle != blank %}
				<div class="{{ subtitle_class }} block-subtitle">
				  <span class="caption-item">{{ item.item_subtitle }}</span>
				</div>
			  {% endif %}
			  
			  {% if item.custom_heading != blank %}
				<div class="custom_large_title block-title">
				  <span class="caption-item">{{ item.custom_heading }}</span>
				</div>
			  {% endif %}
			  
			  {% if item.item_title != blank %}
				<h2 class="{{ title_class }} block-title">
				  <span class="caption-item">{{ item.item_title }}</span>
				</h2>
			  {% endif %}
			  
			  {% if item.item_text != blank %}
				<div class="{{ text_class }} block-text">
				  <div class="caption-item">{{ item.item_text }}</div>
				</div>
			  {% endif %}
			  
			  {% if item.link_text != blank and item.item_link != blank %}
				{% if item.btn_style == 'link-default' or item.btn_style == 'link-primary' %}
				  <a href="{{ item.item_link }}" class="link{% if item.inverted %} link_inverted {% endif %}{% if item.btn_style == 'link-primary' %} link-primary{% else %} link-default{% endif %}">{{ item.link_text }}</a>
				{% else %}
				  <a href="{{ item.item_link }}" class="btn {{ item.btn_style }} {{ item.btn_size }}">{{ item.link_text }}</a>
				{% endif %}
			  {% endif %}
			</div>
		  </div>
		{% endif %}
	  </div>
	  
	  {% when 'video' %}
	  <style>
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .bgvideo-{{ block.id }} {
		padding: {{ item.padding_desktop | append: 'vw' }} 0;
		}
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .bgvideo-{{ block.id }}.bg-zmzvideo .zmz-video video {
		object-position: {{ item.bgvideo_position_x | append: "%" }} 20%;
		}
		
		@media (max-width: 991px) {
		.section-style-{{ section.id }} .item-block--{{ block.id }} .bgvideo-{{ block.id }} {
		padding: {{ item.padding_mobile | append: 'px' }} 0;
		}
		}
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .bgvideo-{{ block.id }}::before {
		content: "";
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		z-index: 1;
		background-color: {{ item.bg_overlay }};
		opacity: {{ item.opacity_bg_overlay }};
		}
		
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .block-title {
		color: {{ item.color_title }};
		}
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .custom_large_title.block-title {
		color: {{ item.color_ch }};
		}
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .block-text {
		color: {{ item.color_text }};
		}
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption {
		position: absolute;
		left: 0;
		right: 0;
		top: 0;
		bottom: 0;
		}
		
		.section-style-{{ section.id }} .item-block--{{ block.id }} .banner-caption:not(.inverted) .block-subtitle {
		color: {{ item.color_subtitle }};
		}
		
		{% if item.use_bgo %}
		  /* Style background block */
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .caption::before {
		  content: '';
		  position: absolute;
		  top: 0;
		  left: 0;
		  right: 0;
		  bottom: 0;
		  display: block;
		  z-index: 1;
		  background-color: {{ item.bg_color | color_modify: 'alpha', item.bg_color_opacity }};
		  transition: .3s;
		  }
		
		{% endif %}
	  
	  </style>
	  
	  {% case block.settings.caption_size %}
	  {% when 'large' %}
	  {%- assign subtitle_class = 'h6' -%}
	  {%- assign title_class = 'h1' -%}
	  {%- assign text_class = 'text-medium' -%}
	  {% when 'medium' %}
	  {%- assign subtitle_class = 'h6' -%}
	  {%- assign title_class = 'h2' -%}
	  {%- assign text_class = 'text-medium' -%}
	  {% when 'xsmall' %}
	  {%- assign title_class = 'h4' -%}
	  {%- assign text_class = 'text-medium' -%}
		{% else %}
	  {%- assign subtitle_class = 'h6' -%}
	  {%- assign title_class = 'h3' -%}
	  {%- assign text_class = 'text-medium' -%}
	  {% endcase %}
	  <div class="section-block">
		<div class="bgvideo-{{ block.id }} bg-zmzvideo" data-block-id="{{ block.id }}" data-block-type="background-video">
		  {% if item.link_video != blank %}
			<div class="zmz-video">
			  <video id="Mp4{{ block.id }}" src="{{ item.link_video }}" loop="" muted="" playsinline="" autoplay=""></video>
			</div>
		  {% else if %}
			<div class="zmz-video">
			  {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
			</div>
		  {% endif %}
		  
		  {% if item.item_subtitle != blank or item.item_title != blank or item.item_text != blank or item.link_text != blank %}
			<div class="banner-caption {% if item.width == '12' %} container{% endif %} caption caption-{{ item.caption_size }} d-inline-flex flex-column text-md-{{ item.textalign_pos }} justify-content-md-{{ item.v_cpos }} align-items-md-{{ item.h_cpos }} {% if item.inverted %} inverted{% endif %}" style="padding: {{ item.inset_caption }};{% if item.link_type_banner and item.item_link != blank and item.banner_style == 'overlay' %} pointer-events: none;{% endif %}">
			  <div class="caption_text">
				{% if item.item_subtitle != blank %}
				  <div class="{{ subtitle_class }} block-subtitle">
					<span class="caption-item">{{ item.item_subtitle }}</span>
				  </div>
				{% endif %} 
				
				{% if item.custom_heading != blank %}
				  <div class="custom_large_title block-title">
					<span class="caption-item">{{ item.custom_heading }}</span>
				  </div>
				{% endif %}
				
				{% if item.item_title != blank %}
				  <h2 class="{{ title_class }} block-title">
					<span class="caption-item">{{ item.item_title }}</span>
				  </h2>
				{% endif %}
				
				{% if item.item_text != blank %}
				  <div class="{{ text_class }} block-text">
					<div class="caption-item">{{ item.item_text }}</div>
				  </div>
				{% endif %}
				
				{% if item.link_text != blank and item.item_link != blank %}
				  {% if item.btn_style == 'link-default' or item.btn_style == 'link-primary' %}
					<a href="{{ item.item_link }}" class="link{% if item.btn_style == 'link-primary' %} link-primary{% else %} link-dark{% endif %}">{{ item.link_text }}</a>
				  {% else %}
					<a href="{{ item.item_link }}" class="btn {{ item.btn_style }} {{ item.btn_size }}">{{ item.link_text }}</a>
				  {% endif %}
				{% endif %}
			  </div>
			</div>
		  {% endif %}
		</div>
	  </div>
	  
	  {% when 'collection' %}
	  <div class="block_wrapper collection_listing__main">
		<style>
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .collection_item::before {
		  content: '';
		  position: absolute;
		  top: 0;
		  left: 0;
		  right: 0;
		  bottom: 0;
		  display: block;
		  z-index: 1;
		  transition: .3s;
		  pointer-events: none;
		  background-color: {{ item.caption_bg_color | color_modify: 'alpha', item.caption_bg_color_opacity }};
		  }
		</style>
		<div class="collection">
		  
		  {% assign collection = collections[item.item_collection] %}
		  {% assign collection_image = false %}
		  {% assign collection_title = false %}
		  {% assign img_alignment = item.img_alignment %}
		  {%- assign item_w = item.item_width | plus: 0 -%}
		  
		  {% if item.col_image_ratio == "portrait" %}
			{% if item_w <= 3 %}
			  {%- assign item_size = '270x340' -%}
			  {% elsif item_w > 3 and item_w <= 5 %}
			  {%- assign item_size = '370x489' -%}
			{% else %}
			  {%- assign item_size = '650x850' -%}
			{% endif %}
		  {% else %}
			{% if item_w <= 3 %}
			  {%- assign item_size = '270x270' -%}
			  {% elsif item_w > 3 and item_w <= 5 %}
			  {%- assign item_size = '370x370' -%}
			{% else %}
			  {%- assign item_size = '650x650' -%}
			{% endif %}
		  {% endif %}
		  
		  
		  {% if item.item_img %}
			{% assign collection_image = true %}
			{% assign collection_image_src = item.item_img %}
		  {% endif %}
		  
		  {% if collection_image %}
			
			{% capture collection_img_url %}{{ collection_image_src | img_url: item_size, crop: img_alignment, format: 'jpg' }}{% endcapture %}
			{% capture collection_img_min_url %}{{ collection_image_src | img_url: '100x100', crop: img_alignment, format: 'jpg' }}{% endcapture %}
			{% elsif collection.image %}
			{% capture collection_img_url %}{{ collection | img_url: item_size, crop: img_alignment, format: 'jpg' }}{% endcapture %}
			{% capture collection_img_min_url %}{{ collection | img_url: '100x100', crop: img_alignment, format: 'jpg' }}{% endcapture %}
			{% elsif collection.products.first %}
			{% assign iteration_count = collection.products_count | minus: 1 %}
			
			{% for i in (0..iteration_count) %}
			  {% if collection.products[i].images.size > 0 %}
				{% capture collection_img_url %}{{ collection.products[i].images.first | img_url: item_size, crop: img_alignment, format: 'jpg' }}{% endcapture %}
				{% capture collection_img_min_url %}{{ collection.products[i].images.first | img_url: '100x100', crop: img_alignment, format: 'jpg' }}{% endcapture %}
				
				{% break %}
			  {% else %}
				{% assign collection_img_url = 0 %}
			  {% endif %}
			{% endfor %}
		  {% else %}
			{% assign collection_img_url = 0 %}
		  {% endif %}
		  
		  <a class="collection_item d-block {% if item.position-caption == 'top' %} position-top{% elsif  item.position-caption == 'bottom' %} position-bottom{% else %} position-center{% endif %}" href="{{ collection.url }}">
			<div class="collection_img">
			  {% if collection_img_url == 0 %}
				<div class="img_placeholder__wrap">{{ 'collection-1' | placeholder_svg_tag }}</div>
			  {% else %}
