			<div class="{% if item.inverted %} inverted{% endif %}">
			  {% include 'blog-grid-item' %}
			</div>
		  {% when 'list' %}
			<div class="{% if item.inverted %} inverted{% endif %}">
			  {% include 'blog-listing-item' %}
			</div>
		  {% endcase %}
		{% else %}
		  <div class="mt-1">
			<div class="article_item no-content mb-2">
			  <div class="card-wrapper">
				<div class="card__inner">
				  <div class="card__media-full-spacer">
					<div class="media media--landscape"></div>
				  </div>
				</div>
			  </div>
			  <div class="article_caption mt-1">
				<div class="article_title h5">
				  <span>Post name</span>
				</div>
				<div class="link link-primary mt-2">
				  <span>Post link url</span>
				</div>
			  </div>
			</div>
		  </div>
		{% endif %}
	  </div>
	  
	  {% when 'list_links' %}
	  {%- assign list = item.links -%}
	  <style>
		{% if item.use_bgo %}
		  
		  /* Style background block */
		  .section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper:before {
		  content: '';
		  position: absolute;
		  top: 0;
		  left: 0;
		  right: 0;
		  bottom: 0;
		  display: block;
		  z-index: 1;
		  background-color: {{ item.bg_color | color_modify: 'alpha', item.bg_color_opacity }};
		  transition: .3s;
		  }
		
		{% endif %}
		.section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper {
		padding: {{ item.inset_caption }};
		}
		
		@media (min-width: 768px) {
		.section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper ul {
		column-count: {{ item.column_count }};
		}
		}
	  </style>
	  <div class="block_wrapper linklist{% if item.inverted %} inverted{% endif %}">
		<ul class="link-list">
		  {% for link in list.links %}
			<li>
			  <a class="link mt-1" href="{{ link.url }}">
				<span>{{ link.title }}</span>
			  </a>
			</li>
		  {% endfor %}
		</ul>
	  </div>
	  {% when 'image' %}
	  <style>
		{% if item.use_bgo %}
        /* Style background block */
        .section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper {
            background-color: {{ item.bg_color | color_modify: 'alpha', item.bg_color_opacity }};
        }
		
		{% endif %}
		
        .section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper {
            padding: {{ item.inset_caption }};
        }

        @media (min-width: 768px) {
            .section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper {
				margin: {{ item.offset_item_block }};
            }
        }

        .section-style-{{ section.id }} .item-block--{{ block.id }} .block_wrapper img {
            border-radius: {{ item.br_img }};
        }
	  </style>
	  <div class="block_wrapper">
		{% if item.image != blank %}
		  <div class="image-item-container">
			<div class="image-item-wrapper" style="padding-top:{{ 1 | divided_by: item.image.aspect_ratio | times: 100 }}%;">
			  {% assign img_url = item.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
			  <img loading="lazy" class="image-content__image lazyload"
				   src="{{ item.image | img_url: '300x300' }}"
				   data-src="{{ img_url }}"
				   data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
				   data-aspectratio="{{ item.image.aspect_ratio }}"
				   data-sizes="auto"
				   alt="{{ item.image.alt | escape }}">
			</div>
		  </div>
		
		{% else %}
		  <div class="image-item-container">
			<div class="img_placeholder__wrap" style="padding-top: 52.6%;">
			  {{ 'image' | placeholder_svg_tag: 'img_placeholder' }}
			</div>
		  </div>
		{% endif %}
	  </div>
	  
	   {% when 'lookbook' %}
	  <div class="block_wrapper">
		<div class="lookbook_item_wrap">
		  <div class="lookbook_img">
			{%- if item.image != blank -%}
			  <div class="image-item-wrapper" style="padding-top:{{ 1 | divided_by: item.image.aspect_ratio | times: 100 }}%;">
				{% assign img_url = item.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
				<img loading="lazy" class="image-content__image lazyload"
					 src="{{ item.image | img_url: '300x300' }}"
					 data-src="{{ img_url }}"
					 data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
					 data-aspectratio="{{ item.image.aspect_ratio }}"
					 data-sizes="auto"
					 data-parent-fit="cover"
					 alt="{{ item.image.alt | escape }}"/>
			  </div>
			{%- else -%}
			  <div class="img_placeholder__wrap" style="padding-top: 52.6%;">
				{{ 'image' | placeholder_svg_tag: 'img_placeholder' }}
			  </div>
			{%- endif -%}
		  </div>
		  {%- for i in (1..4) -%}
			{%- assign spotX = 'item_x_' | append: i -%}{%- assign spotX = item[spotX] -%}
			{%- assign spotY = 'item_y_' | append: i -%}{%- assign spotY = item[spotY] -%}
			{%- assign spotProduct = 'item_product_' | append: i -%}{%- assign spotProduct = item[spotProduct] -%}
			{%- unless spotProduct == blank -%}
			{%- assign itemProduct = all_products[spotProduct] -%}
			  <div class="lookbook_container">
				<div class="lookbook_item" style="left: {{ spotX }}%;top: {{ spotY }}%;">
				  {%- if itemProduct.id.size > 0 -%}
					<button onclick="openPopover(event, 'popover-id-{{ block.id }}_{{ i }}')" class="lookbook_item__bullet" style="position:absolute; left: {{ spotX }}%;top: {{ spotY }}%;" data-popover="{{ block.id }}_{{ i }}" data-toggle='popover'>
					  <i aria-hidden="true">
						{% render 'icon-plus' %}
					  </i>
					</button>
					<div id="popover-id-{{ block.id }}_{{ i }}" role="tooltip" data-popover="{{ block.id }}_{{ i }}" class="lookbook_item__caption hidden {{ item.product_style }}">
					  {% if item.product_style == "style_1" %}
						<div class="product_listing__main product_listing__grid mx-0">
						  <div class="product_item_wrap">
							{% render 'product-listing-item',
							  product_card_product: itemProduct,
							  pc_grid_style: "grid_style_1",
							  media_size: settings.image_ratio
							%}
						  </div>
						</div>
					  {% else %}
						<div class="product_item_wrap list_products product_listing__list">
						  {% render 'product-item-simple',
							product_card_product: itemProduct,
							media_size: settings.image_ratio %}
						</div>
					  {% endif %}
					</div>
				
				  {%- else -%}
					<div class="lookbook_item__bullet">
					  <i aria-hidden="true">
						{% render 'icon-plus' %}
					  </i>
					</div>
				  {%- endif -%}
				</div>
			  </div>
			{%- endunless -%}
		  {%- endfor -%}
		</div>
	  </div>
	  <script src="{{ 'popover.js' | asset_url }}"></script>
	  <script type="text/javascript">
          // button popover clicked
          window.openPopover = function openPopover(event, popoverID) {
              let element = event.target;
              while (element.nodeName !== "BUTTON") {
                  element = element.parentNode;
              }
              Popper.createPopper(element, document.getElementById(popoverID), {
                  placement: "auto",
                  modifiers: [
                      {
                          name: "offset",
                          options: {
                              offset: [0, 6]
                          }
                      },
                      {
                          name: "flip",
                          options: {
                              allowedAutoPlacements: ["left","right","top", "bottom"],
                              rootBoundary: "viewport"
                          }
                      }
                  ]
              });
              element.classList.toggle("is-active");
              document.getElementById(popoverID).classList.toggle("hidden");
              document.getElementById(popoverID).classList.toggle("is-open");
          }
          $(document).ready(function () {
              // Hide content popup after clicked outside target
              $('body').on('click', function (e) {
                  let elBul = $("[data-toggle='popover']");
                  let elActiveBul = $("[data-toggle='popover'].is-active");
                  elActiveBul.each(function () {
                      let elemThis = $(this);
                      // hide any open popovers when the anywhere else in the body is clicked
                      if (!elemThis.is(e.target) && !elBul.is(e.target) && elemThis.has(e.target).length === 0 && $('.lookbook_item__caption.is-open').has(e.target).length === 0) {
                          elemThis.click();
                          elemThis.removeClass('is-active');
                      }
                  });
              });
          });
	  </script>
	  {% endcase %}
	</div>
	{% endfor %}
  </div>
</div>
</div>


